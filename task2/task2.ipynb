    "class AsyncDownloadGif:\n",
    "    def __init__(\n",
    "        self,\n",
    "        url: str,\n",
    "        number_of_gifs: int,\n",
    "        host_name: str,\n",
    "        port: int,\n",
    "        user_name: str,\n",
    "        key_path: str, \n",
    "        path_directory: str,\n",
    "    ):\n",
    "        self.number_of_gifs = number_of_gifs\n",
    "        self.url = url\n",
    "        self.path_directory = path_directory\n",
    "        self.host_name = host_name\n",
    "        self.port = port\n",
    "        self.user_name = user_name\n",
    "        self.key_path = key_path\n",
    "\n",
    "    async def init_session_with_ssh_key(self) -> paramiko.SSHClient:\n",
    "        self.ssh = paramiko.SSHClient()\n",
    "        try:\n",
    "            self.ssh.load_system_host_keys()\n",
    "            self.ssh.set_missing_host_key_policy(paramiko.AutoAddPolicy())\n",
    "            self.ssh.connect(hostname=self.host_name, port=self.port, username=self.user_name, key_filename=self.key_path)\n",
    "\n",
    "        except Exception as e:\n",
    "            print(f'Error massage: {e}')\n",
    "            return None\n",
    "\n",
    "    async def download_gif(self, session, index):\n",
    "        try:\n",
    "            async with session.get(self.url) as response:\n",
    "                if response.status == 200:\n",
    "                    gif_content = await response.read()\n",
    "                    gif_file_path = f'{self.path_directory}/gif_{index}.gif'\n",
    "                    await self.upload_gif_in_services(gif_content, gif_file_path)\n",
    "                else:\n",
    "                    print(f'status code: {response.status}')\n",
    "        except Exception as e:\n",
    "            print(f'Error message: {e}')\n",
