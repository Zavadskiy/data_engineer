    "    #             await file.write(content)\n",
    "    #     except Exception as e:\n",
    "    #         print(f'Error massage: {e}')\n",
    "\n",
    "    async def upload_gif_in_services(self, content: bytes, gif_file_path: str):\n",
    "        if self.ssh:\n",
    "            try:\n",
    "                sftp = self.ssh.open_sftp()\n",
    "                async with sftp.file(gif_file_path, 'wb') as file:\n",
    "                    await file.write(content)\n",
    "            except Exception as e:\n",
    "                print(f'Error uploading file: {e}')\n",
    "        else:\n",
    "            print('SSH connection is not established.')\n",
    "    \n",
    "    async def main(self):\n",
    "        start_time = time.time()\n",
    "        await self.init_session_with_ssh_key()\n",
    "        async with aiohttp.ClientSession() as session:\n",
    "            tasks = [self.download_gif(session, i) for i in range(self.number_of_gifs)]\n",
    "            await asyncio.gather(*tasks)\n",
    "        end_time = time.time()\n",
    "        script_time = end_time - start_time\n",
    "        print(f\"time script: {script_time} sec\")"
