    "import requests\n",
    "import time\n",
    "from argparse import ArgumentParser\n",
    "import paramiko\n",
    "from argparse import Namespace\n",
    "\n",
    "\n",
    "class DownloadGift:\n",
    "    def __init__(\n",
    "        self,\n",
    "        url: str,\n",
    "        number_of_gifs: int,\n",
    "        host_name: str,\n",
    "        port: int,\n",
    "        user_name: str,\n",
    "        key_path: str, \n",
    "        path_directory: str,\n",
    "    ):\n",
    "        self.number_of_gifs = number_of_gifs\n",
    "        self.url = url\n",
    "        self.path_directory = path_directory\n",
    "        self.host_name = host_name\n",
    "        self.port = port\n",
    "        self.user_name = user_name\n",
    "        self.key_path = key_path\n",
    "\n",
    "    def init_session_with_ssh_key(self) -> paramiko.SSHClient:\n",
    "        self.ssh = paramiko.SSHClient()\n",
    "        try:\n",
    "            self.ssh.load_system_host_keys()\n",
    "            self.ssh.set_missing_host_key_policy(paramiko.AutoAddPolicy())\n",
    "            self.ssh.connect(hostname=self.host_name, port=self.port, username=self.user_name, key_filename=self.key_path)\n",
    "\n",
    "        except Exception as e:\n",
    "            print(f'Error massage: {e}')\n",
    "            return None\n",
    "\n",
    "    def download_gifs(self):\n",
    "        start_time = time.time()\n",
    "        self.init_session_with_ssh_key()\n",
    "        try:\n",
    "            for i in range(self.number_of_gifs):\n",
    "                url = self.url\n",
    "                response = requests.get(url)\n",
    "                \n",
    "                if response.status_code == 200:\n",
    "                    gif_content = response.content\n",
    "                    gif_file_path = f'{self.path_directory}/gif_{i}.gif'\n",
    "                    self.upload_gif_in_services(gif_content, gif_file_path)\n",
    "                else:\n",
    "                    print(f'status code: {response.status}')\n",
    "        except Exception as e:\n",
    "            print(f'Error massage: {e}')\n",
    "\n",
    "        end_time = time.time()\n",
    "        script_time = end_time - start_time\n",
    "        return script_time\n",
    "\n",
    "    # def upload_gif_in_services(self, content: bytes, gif_file_path: str):\n",
    "    #     try:\n",
    "    #         with open(gif_file_path, 'wb') as file_path:\n",
    "    #             file_path.write(content)\n",
    "    #     except Exception as e:\n",
    "    #         print(f'Error massage: {e}')\n",
    "            \n",
    "    def upload_gif_in_services(self, content: bytes, gif_file_path: str):\n",
    "        if self.ssh:\n",
    "            try:\n",
    "                with paramiko.SFTPClient.from_transport(self.ssh.get_transport()) as scp:\n",
    "                    with scp.open(gif_file_path, 'wb') as file_path:\n",
    "                        file_path.write(content)\n",
    "            except Exception as e:\n",
    "                print(f'Error massage: {e}')\n",
    "        else:\n",
    "            print('SSH connection is not established.')"
